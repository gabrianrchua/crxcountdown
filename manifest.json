{
    "name": "CRXCountdown",
    "description": "Add a countdown to any tab you're working in.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/img/icon_128.png"
        }
    },
    "permissions": ["storage"],
    "icons": {
        "128": "/img/icon_128.png"
    }
}